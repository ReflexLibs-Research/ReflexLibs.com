<Inflation>

  <!-- AUS : Australia -->
  <!-- Data from http://www.abs.gov.au -->
  <!-- The publication dates are estimated in this data, the AUS CPI site claims they are published 4th Wednesday after apply date -->
  <!-- This data corresponds to the "All groups Australia index - Series ID A2325846C -->
  <InflationIndex>
    <Identifier>
      <Country>AUS</Country>
      <Name>CPI</Name>
    </Identifier>

    <Attributes>
      <Attribute>
        <Name>Website</Name>
        <Value>www.abs.gov.au</Value>
      </Attribute>
      <Attribute>
        <Name>Description</Name>
        <Value>All groups Australia index</Value>
      </Attribute>
      <Attribute>
        <Name>Series</Name>
        <Value>A2325846C</Value>
      </Attribute>
    </Attributes>

    <!-- Base Year was changed to average of 2011-2012 index publications -->
    <BaseValue>100.0</BaseValue>
    <IndexMonths>
      <Month>Mar</Month>
      <Month>Jun</Month>
      <Month>Sep</Month>
      <Month>Dec</Month>
    </IndexMonths>

    <AnnouncementAdjuster>
      <Tenor>1M</Tenor>
      <Tenor>15D</Tenor>
      <RollForwardIfNotBusinessDay>
        <Calendar>SYB</Calendar>
      </RollForwardIfNotBusinessDay>
    </AnnouncementAdjuster>
  </InflationIndex>


  <!-- CAN : Canada -->

  <InflationIndex>

    <Identifier>
      <Country>CAN</Country>
      <Name>CPI</Name>
    </Identifier>

    <Attributes>
      <Attribute>
        <Name>Website</Name>
        <Value>www.statcan.gc.ca</Value>
      </Attribute>
      <Attribute>
        <Name>Description</Name>
        <Value>Canadian CPI</Value>
      </Attribute>
      <Attribute>
        <Name>Series</Name>
        <Value>v41690973</Value>
      </Attribute>
    </Attributes>

    <!-- The CPI index was re-based in Jun 2007, see http://www.bankofcanada.ca/2007/06/change-official-time-base-reference/
       The base index is not for a month, historic inflation values for 2002 were rescaled so that the average value for the 12
       2002 values was 100
   -->
    <BaseValue>100.0</BaseValue>
    <IndexMonths>
      <Month>Jan</Month>
      <Month>Feb</Month>
      <Month>Mar</Month>
      <Month>Apr</Month>
      <Month>May</Month>
      <Month>Jun</Month>
      <Month>Jul</Month>
      <Month>Aug</Month>
      <Month>Sep</Month>
      <Month>Oct</Month>
      <Month>Nov</Month>
      <Month>Dec</Month>
    </IndexMonths>

    <!-- Friday after third Wednesday -->
    <AnnouncementAdjuster>
      <Tenor>1M</Tenor>
      <DayOfWeek>
        <Day>Wednesday</Day>
        <NumberOfWeeks>3</NumberOfWeeks>
      </DayOfWeek>
      <DayOfWeek>
        <Day>Friday</Day>
        <NumberOfWeeks>0</NumberOfWeeks>
      </DayOfWeek>
      <RollForwardIfNotBusinessDay>
        <Calendar>TRB</Calendar>
      </RollForwardIfNotBusinessDay>
    </AnnouncementAdjuster>
  </InflationIndex>

